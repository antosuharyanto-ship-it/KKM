{
    "rewrites": [
        {
            "source": "/api/:match*",
            "destination": "https://kkm1-production.up.railway.app/api/:match*"
        },
        {
            "source": "/auth/:match*",
            "destination": "https://kkm1-production.up.railway.app/auth/:match*"
        },
        {
            "source": "/tickets/:match*",
            "destination": "https://kkm1-production.up.railway.app/tickets/:match*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}